[
    { "key": "alt+1", "command": "workbench.action.focusFirstEditorGroup" },
    { "key": "alt+2", "command": "workbench.action.focusSecondEditorGroup" },
    { "key": "alt+3", "command": "workbench.action.focusThirdEditorGroup" },
    { "key": "alt+[", "command": "workbench.action.previousEditor" },
    { "key": "alt+]", "command": "workbench.action.nextEditor" },
    { "key": "cmd+1", "command": "workbench.action.openEditorAtIndex1" },
    { "key": "cmd+2", "command": "workbench.action.openEditorAtIndex2" },
    { "key": "cmd+3", "command": "workbench.action.openEditorAtIndex3" },
    { "key": "cmd+4", "command": "workbench.action.openEditorAtIndex4" },
    { "key": "cmd+5", "command": "workbench.action.openEditorAtIndex5" },
    { "key": "cmd+6", "command": "workbench.action.openEditorAtIndex6" },
    { "key": "cmd+7", "command": "workbench.action.openEditorAtIndex7" },
    { "key": "cmd+8", "command": "workbench.action.openEditorAtIndex8" },
    {
        "key": "cmd+9",
        "command": "workbench.action.openEditorAtIndex9"
    },
    {
        "key": "cmd+r",
        "command": "workbench.action.gotoSymbol"
    },
    {
        "key": "shift+cmd+o",
        "command": "-workbench.action.gotoSymbol"
    },
    {
        "key": "cmd+enter",
        "command": "workbench.action.toggleMaximizedPanel"
    },
    {
        "key": "ctrl+i",
        "command": "python.sortImports"
    },
    {
        "key": "ctrl+u",
        "command": "editor.action.transformToUppercase"
    },
    {
        "key": "ctrl+s",
        "command": "editor.action.sortLinesAscending"
    },
    {
        "key": "ctrl+shift+o",
        "command": "workbench.action.output.toggleOutput"
    },
    {
        "key": "shift+cmd+u",
        "command": "-workbench.action.output.toggleOutput"
    },
    {
        "key": "ctrl+p",
        "command": "workbench.actions.view.problems"
    },
    {
        "key": "shift+cmd+m",
        "command": "-workbench.actions.view.problems"
    },
    {
        "key": "shift+cmd+g",
        "command": "workbench.view.scm"
    },
    {
        "key": "ctrl+shift+g",
        "command": "-workbench.view.scm"
    },
    {
        "key": "ctrl+`",
        "command": "workbench.action.terminal.focus",
        "when": "!terminalFocus"
    },
    {
        "key": "ctrl+o",
        "command": "python.viewTestOutput",
        "when": "editorLangId == python"
    }
]
